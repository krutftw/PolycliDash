PORT=4321

# Polymarket CLI executable name/path.
CLI_BINARY=polymarket

# Optional path to presets file (relative to project root).
CLI_PRESETS_PATH=config/cli-presets.json

# Command timeout in milliseconds.
CLI_COMMAND_TIMEOUT_MS=25000

# Ollama endpoint (local or cloud-hosted).
OLLAMA_BASE_URL=http://localhost:11434

# Mandatory: must start with glm-5.
OLLAMA_MODEL=glm-5

